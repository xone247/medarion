{
  "name": "medarion-healthcare-platform",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "cd server && npm start",
    "server:dev": "cd server && npm run dev",
    "install:all": "npm install && cd server && npm install",
    "start": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "setup": "npm run install:all && npm run setup:db",
    "setup:db": "node setup-database.js",
    "migrate:data": "node migrate-data-simple.js",
    "setup:full": "npm run setup:db && npm run migrate:data",
    "capture:media:install": "npm i -D playwright && npx playwright install",
    "capture:media": "node ./scripts/capture_media.mjs",
    "seed:glossary": "cd server && node seed_glossary_terms.js"
  },
  "dependencies": {
    "@tinymce/tinymce-react": "^6.3.0",
    "@types/react-router-dom": "^5.3.3",
    "bcryptjs": "^3.0.2",
    "lucide-react": "^0.539.0",
    "mysql2": "^3.6.5",
    "quill": "^2.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-quill": "^2.0.0",
    "react-router-dom": "^7.9.4",
    "recharts": "^3.1.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/http-proxy": "^1.17.17",
    "@types/node": "^22.8.6",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "http-proxy": "^1.18.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.1"
  }
}
